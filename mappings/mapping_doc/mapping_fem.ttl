map:person_000 dct:contributor foaf:Person ;
	rdfs:label "Jhon Toledo" ;
	foaf:mbox <mailto:ja.toledo@upm.es> .
map:rules_000 schema:contributor map:person_000 ;
	rdf:type schema:Dataset;
    schema:version "1.0.0";
    schema:title "Mappings RML para transformar el Censo/Registro de Vehículos a RDF (EDINT CensoVehículos)";
    schema:dateCreated "14-02-2026";
    schema:description "Este conjunto de mappings RML transforma los datos de un censo o registro municipal de vehículos a RDF, utilizando la ontología EDINT CensoVehículos. El objetivo es generar un grafo RDF interoperable para representar las entidades principales del registro de vehículos y su información asociada, orientada a usos habituales de las entidades locales".


<owner_0> a rr:TriplesMap;

	rml:logicalSource [
		a rml:LogicalSource;
		rml:source "dataset_enriquecido.csv";
		rml:referenceFormulation ql:CSV
	];
	rr:subjectMap [
		a rr:SubjectMap;
		rr:template "http://vocab.linkeddata.es/datosabiertos/recurso/titular/{company}~iri";
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant rdf:type;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rr:constant edintveh:Owner;
		];
	].


<permit_0> a rr:TriplesMap;

	rml:logicalSource [
		a rml:LogicalSource;
		rml:source "dataset_enriquecido.csv";
		rml:referenceFormulation ql:CSV
	];
	rr:subjectMap [
		a rr:SubjectMap;
		rr:template "http://vocab.linkeddata.es/datosabiertos/recurso/autorizacion/{subtipo_autorizacion}-{estado}-{fecha_inicio_periodo}-{fecha_activacion}-{fecha_vigencia}-{distintivo}-{tipologia_vehiculo}-{periodicidad}";
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant rdf:type;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rr:constant edintveh:Permit;
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant edintveh:authorisesInArea;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rr:template "http://vocab.linkeddata.es/datosabiertos/recurso/trafico/zona-regulada/{zona-regulada_id}";
			rr:termType rr:IRI
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant edintveh:authorisesInSegment;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rr:template "http://vocab.ciudadesabiertas.es/recurso/transporte/trafico/tramo/{tramo_id}";
			rr:termType rr:IRI
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant edintveh:permitType;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rr:template "http://vocab.linkeddata.es/datosabiertos/kos/edint/VehiclePermitType/{tipo_autorizacion}";
			rr:termType rr:IRI
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant edintveh:permitStatus;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rr:template "http://vocab.linkeddata.es/datosabiertos/kos/edint/VehiclePermitStatus/{estado}";
			rr:termType rr:IRI
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant edintveh:permitPeriodicity;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rr:template "http://vocab.linkeddata.es/datosabiertos/kos/edint/VehiclePermitPeriodicity/{periodicidad}";
			rr:termType rr:IRI
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant edintveh:permitOwner;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rr:template "http://vocab.linkeddata.es/datosabiertos/recurso/titular/{company}";
			rr:termType rr:IRI
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant edintveh:authorisesVehicle;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rr:template "http://vocab.linkeddata.es/datosabiertos/transporte/recurso/vehiculo/{vehiculo_id}";
			rr:termType rr:IRI
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant edintveh:beginningDate;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rml:reference "fecha_inicio_periodo";
			rr:datatype xsd:date
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant edintveh:activationDate;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rml:reference "fecha_activacion";
			rr:datatype xsd:date
		];
	].


