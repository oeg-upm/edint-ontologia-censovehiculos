@prefix edintvehicletype: <http://vocab.linkeddata.es/datosabiertos/kos/edint/VehicleType/>.
@prefix edintvehiclepropulsiontype: <http://vocab.linkeddata.es/datosabiertos/kos/edint/VehiclePropulsionType/>.
@prefix edintvehicleenvironmentalbadge: <http://vocab.linkeddata.es/datosabiertos/kos/edint/VehicleEnvironmentalBadge/>.
@prefix edintveh: <http://vocab.linkeddata.es/datosabiertos/def/transporte/censovehiculos#>.
@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix d2rq: <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix schema: <http://schema.org/>.
@prefix formats: <http://www.w3.org/ns/formats/>.
@prefix comp: <http://semweb.mmlab.be/ns/rml-compression#>.
@prefix void: <http://rdfs.org/ns/void#>.
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#>.
@prefix grel: <http://users.ugent.be/~bjdmeest/function/grel.ttl#>.
@base <http://example.com/ns#>.


<vehicle_0> a rr:TriplesMap;

	rml:logicalSource [
		a rml:LogicalSource;
		rml:source "dataset_enriquecido.csv";
		rml:referenceFormulation ql:CSV
	];
	rr:subjectMap [
		a rr:SubjectMap;
		rr:template "http://vocab.linkeddata.es/datosabiertos/transporte/recurso/vehiculo/{vehiculo_id}";
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant rdf:type;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rr:constant edintveh:Vehicle;
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant edintveh:registeredOwner;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rr:template "http://vocab.linkeddata.es/datosabiertos/recurso/titular/{company}";
			rr:termType rr:IRI
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant edintveh:vehicleType;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rr:template "http://vocab.linkeddata.es/datosabiertos/kos/edint/VehicleType/{tipologia_vehiculo}";
			rr:termType rr:IRI
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant edintveh:propulsionType;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rr:template "http://vocab.linkeddata.es/datosabiertos/kos/edint/VehiclePropulsionType/{tipologia_propulsion}";
			rr:termType rr:IRI
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant edintveh:environmentalBadge;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rr:template "http://vocab.linkeddata.es/datosabiertos/kos/edint/VehicleEnvironmentalBadge/{distintivo}";
			rr:termType rr:IRI
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant edintveh:licensePlateNumber;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rml:reference "vehiculo_id";
			rr:datatype xsd:string
		];
	].


