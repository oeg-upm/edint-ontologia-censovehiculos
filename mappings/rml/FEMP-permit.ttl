@prefix edintvehiclepermittype: <http://vocab.linkeddata.es/datosabiertos/kos/edint/VehiclePermitType/>.
@prefix edintvehiclepermitstatus: <http://vocab.linkeddata.es/datosabiertos/kos/edint/VehiclePermitStatus/>.
@prefix edintvehiclepermitperiodicity: <http://vocab.linkeddata.es/datosabiertos/kos/edint/VehiclePermitPeriodicity/>.
@prefix edintveh: <http://vocab.linkeddata.es/datosabiertos/def/transporte/censovehiculos#>.
@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix d2rq: <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix schema: <http://schema.org/>.
@prefix formats: <http://www.w3.org/ns/formats/>.
@prefix comp: <http://semweb.mmlab.be/ns/rml-compression#>.
@prefix void: <http://rdfs.org/ns/void#>.
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#>.
@prefix grel: <http://users.ugent.be/~bjdmeest/function/grel.ttl#>.
@base <http://example.com/ns#>.


<permit_0> a rr:TriplesMap;

	rml:logicalSource [
		a rml:LogicalSource;
		rml:source "dataset_enriquecido.csv";
		rml:referenceFormulation ql:CSV
	];
	rr:subjectMap [
		a rr:SubjectMap;
		rr:template "http://vocab.linkeddata.es/datosabiertos/recurso/autorizacion/{id_autorizacion}";
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant rdf:type;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rr:constant edintveh:Permit;
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant edintveh:authorisesInArea;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rr:template "http://vocab.linkeddata.es/datosabiertos/recurso/trafico/zona-regulada/{zona-regulada_id}";
			rr:termType rr:IRI
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant edintveh:authorisesInSegment;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rr:template "http://vocab.ciudadesabiertas.es/recurso/transporte/trafico/tramo/{tramo_id}";
			rr:termType rr:IRI
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant edintveh:permitType;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rr:template "http://vocab.linkeddata.es/datosabiertos/kos/edint/VehiclePermitType/{tipo_autorizacion}";
			rr:termType rr:IRI
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant edintveh:permitStatus;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rr:template "http://vocab.linkeddata.es/datosabiertos/kos/edint/VehiclePermitStatus/{estado}";
			rr:termType rr:IRI
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant edintveh:permitPeriodicity;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rr:template "http://vocab.linkeddata.es/datosabiertos/kos/edint/VehiclePermitPeriodicity/{periodicidad}";
			rr:termType rr:IRI
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant edintveh:permitOwner;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rr:template "http://vocab.linkeddata.es/datosabiertos/recurso/titular/{company}";
			rr:termType rr:IRI
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant edintveh:authorisesVehicle;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rr:template "http://vocab.linkeddata.es/datosabiertos/transporte/recurso/vehiculo/{vehiculo_id}";
			rr:termType rr:IRI
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant edintveh:beginningDate;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rml:reference "fecha_inicio_periodo";
			rr:datatype xsd:date
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant edintveh:activationDate;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rml:reference "fecha_activacion";
			rr:datatype xsd:date
		];
	].


